<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>registration.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>create</servlet-name>
        <servlet-class>registration.create</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>join</servlet-name>
        <servlet-class>registration.join</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>rolldice</servlet-name>
        <servlet-class>registration.rolldice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>boarddb</servlet-name>
        <servlet-class>registration.boarddb</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sesschech_create</servlet-name>
        <servlet-class>registration.sesschech_create</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signout</servlet-name>
        <servlet-class>registration.signout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sesschck_join</servlet-name>
        <servlet-class>registration.sesschck_join</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>create</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>join</servlet-name>
        <url-pattern>/join</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>rolldice</servlet-name>
        <url-pattern>/rolldice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>boarddb</servlet-name>
        <url-pattern>/boarddb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sesschech_create</servlet-name>
        <url-pattern>/sesschech_create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signout</servlet-name>
        <url-pattern>/signout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sesschck_join</servlet-name>
        <url-pattern>/sesschck_join</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
